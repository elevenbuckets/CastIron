{
  "name": "CastIron",
  "version": "0.1.0",
  "description": "",
  "main": "main.js",
  "directories": {
    "test": "test"
  },
  "babel": {
    "plugins": ["transform-class-properties", "transform-object-rest-spread"],
    "presets": [ [ "env", { "targets": { "node": "8.6" } } ] ]
  },
  "scripts": {
    "postinstall": "npm run core",
    "test": "echo \"Error: no test specified\" && exit 1",
    "w": "./node_modules/.bin/babel ./core/classes/wrapper.js -o ./core/Wrap3.js",
    "q": "./node_modules/.bin/babel ./core/classes/queues.js -o ./core/JobQueue.js",
    "wt": "./node_modules/.bin/babel ./core/classes/bases.js -o ./core/Wallet.js",
    "c": "./node_modules/.bin/babel ./core/classes/casts.js -o ./core/CastIron.js",
    "core": "npm run w && npm run q && npm run wt && npm run c",
    "clean": "rm -vfr ./core/*.js ./dist",
    "dist": "npm run clean && npm run core && mkdir -p ./dist && cp -rv ./core ./libs ./package.json ./dist/"
  },
  "keywords": [],
  "author": "",
  "license": "GPLv3",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "bignumber.js": "^5.0.0",
    "buttercup": "^1.6.2",
    "node-fetch": "^1.7.3",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-dropdown": "^1.4.0",
    "reflux": "^6.4.1",
    "truffle-default-builder": "^2.0.0",
    "uuid": "^3.1.0",
    "web3": "^0.19"
  }
}
